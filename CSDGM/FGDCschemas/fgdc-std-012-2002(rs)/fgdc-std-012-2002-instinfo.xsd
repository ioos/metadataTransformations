				<xs:attribute name="mdname" fixed="Scan Spectral Properties"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="scsppropType">
		<xs:sequence>
			<xs:element ref="specinfo"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="specinfo" type="specinfoType">
		<xs:annotation>
			<xs:documentation>Wavelength-dependent properties of optical systems.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Mandatory"/>
				<xs:attribute name="mdname" fixed="Spectral Information"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="specinfoType" mixed="true">
		<xs:sequence>
			<xs:element ref="nowvband"/>
			<xs:element ref="wvbandpr" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="nowvband" type="nowvbandType">
		<xs:annotation>
			<xs:documentation>Number of separate wavelength ranges at which system measures.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Mandatory"/>
				<xs:attribute name="mdname" fixed="Number of Wavelength Bands"/>
				<xs:attribute name="other-validation-rules" fixed="checkRangeExcludeMin([numbands,0])"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="nowvbandType">
		<xs:sequence>
			<xs:element ref="numbands"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="wvbandpr" type="wvbandprType">
		<xs:annotation>
			<xs:documentation>Design specifications for properties of an individual wavelength range.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Mandatory"/>
				<xs:attribute name="mdname" fixed="Wavelength Band Properties"/>
				<xs:attribute name="other-validation-rules" fixed="checkRangeExcludeMin([lambdmin,0.0]);checkGreaterOrEqual([lambdmax,lambdmin]);checkSpecialRange([pkwavlen,lambdmin,lambdmax])"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="wvbandprType">
		<xs:sequence>
			<xs:element ref="waveunit"/>
			<xs:element ref="bbnddef" minOccurs="0"/>
			<xs:element ref="lambdmin"/>
			<!-- TODO: the following should actually be greater than the previous -->
			<xs:element ref="lambdmax"/>
			<!-- TODO: the rules of the following should be
                            lambdmin <= pkwavlen <= lambdmax -->
			<xs:element ref="pkwavlen" minOccurs="0"/>
			<xs:element ref="wavlreg" minOccurs="0"/>
			<xs:element ref="nomspres" minOccurs="0"/>
			<xs:element ref="bandqual" minOccurs="0"/>
			<xs:element ref="polrzcha" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="banddesc" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="waveunit" type="waveunitType">
		<xs:annotation>
			<xs:documentation>Units in which band wavelengths are expressed.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Mandatory"/>
				<xs:attribute name="mdname" fixed="Wavelength Units"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="waveunitType">
		<xs:union memberTypes="FGDCstring">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="m"/>
					<xs:enumeration value="cm"/>
					<xs:enumeration value="mm"/>
					<xs:enumeration value="&#181;m"/>
					<xs:enumeration value="nm"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:element name="bbnddef" type="bbnddefType">
		<xs:annotation>
			<xs:documentation>Designation of criterion for defining maximum and minimum wavelengths for a spectral band.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Optional"/>
				<xs:attribute name="mdname" fixed="Band Boundary Definition"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="bbnddefType">
		<xs:union memberTypes="FGDCstring">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="3db"/>
					<xs:enumeration value="half maximum"/>
					<xs:enumeration value="50 %"/>
					<xs:enumeration value="1/e"/>
					<xs:enumeration value="equivalent width "/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:element name="lambdmin" type="positiveDoubleType">
		<xs:annotation>
			<xs:documentation>Minimum wavelength boundary of the spectral range of the band using the criterion in Band Boundary Definition, in units specified by Wavelength Units.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Mandatory"/>
				<xs:attribute name="mdname" fixed="Minimum Wavelength"/>
				<xs:attribute name="string-input-format" fixed="Minimum Wavelength &gt; 0.0"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="lambdmax" type="positiveDoubleType">
		<xs:annotation>
			<xs:documentation>Maximum wavelength boundary of the spectral range of the band using the criterion in Band Boundary Definition, in units specified by Wavelength Units.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Mandatory"/>
				<xs:attribute name="mdname" fixed="Maximum Wavelength"/>
				<xs:attribute name="string-input-format" fixed="Maximum Wavelength &gt;= Minimum Wavelength"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="pkwavlen" type="positiveDoubleType">
		<xs:annotation>
			<xs:documentation>Wavelength of maximum sensitivity within the band, in units specified by Wavelength Units.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Optional"/>
				<xs:attribute name="mdname" fixed="Peak Wavelength"/>
				<xs:attribute name="string-input-format" fixed="Minimum Wavelength &lt;= Peak Wavelength &lt;= Maximum Wavelength"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="wavlreg" type="wavlregType">
		<xs:annotation>
			<xs:documentation>Wavelength of maximum sensitivity within the band, in units specified by Wavelength Units.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Optional"/>
				<xs:attribute name="mdname" fixed="Wavelength Region"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="wavlregType">
		<xs:union memberTypes="FGDCstring">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="ultraviolet"/>
					<xs:enumeration value="visible"/>
					<xs:enumeration value="infrared"/>
					<xs:enumeration value="microwave"/>
					<xs:enumeration value="radio"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:element name="nomspres" type="nomspresType">
		<xs:annotation>
			<xs:documentation>Smallest distance between which separate points can be distinguished, as specified in instrument design.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Optional"/>
				<xs:attribute name="mdname" fixed="Nominal Spatial Resolution"/>
				<xs:attribute name="other-validation-rules" fixed="checkRangeExcludeMin([spatresv,0.0])"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="nomspresType">
		<xs:sequence>
			<xs:element ref="spatresu"/>
			<xs:element ref="spatresv"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="spatresu" type="spatresuType">
		<xs:annotation>
			<xs:documentation>Units in which spatial resolution is expressed.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Mandatory"/>
				<xs:attribute name="mdname" fixed="Spatial Resolution Units"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="spatresuType">
		<xs:union memberTypes="FGDCstring">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="meters"/>
					<xs:enumeration value="kilometers"/>
					<xs:enumeration value="degrees"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:element name="spatresv" type="positiveDoubleType">
		<xs:annotation>
			<xs:documentation>Quantitative measure of designed instrument spatial resolution, in units given by Spatial Resolution Units.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Mandatory"/>
				<xs:attribute name="mdname" fixed="Spatial Resolution Value"/>
				<xs:attribute name="string-input-format" fixed="Spatial Resolution Value &gt; 0.0"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="bandqual" type="FGDCstring">
		<xs:annotation>
			<xs:documentation>Description of state of band, including degree of degradation and impact on resolution and measurement accuracy.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Optional"/>
				<xs:attribute name="mdname" fixed="Band Quality"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="polrzcha" type="polrzchaType">
		<xs:annotation>
			<xs:documentation>Degree of polarization of band.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Mandatory-if-applicable"/>
				<xs:attribute name="mdname" fixed="Polarization Characteristics"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="polrzchaType">
		<xs:sequence>
			<xs:element ref="revrpolr"/>
			<xs:element ref="sendpolr" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="revrpolr" type="revrpolrType">
		<xs:annotation>
			<xs:documentation>Polarization direction that a receiver is designed to accept.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Mandatory"/>
				<xs:attribute name="mdname" fixed="Receiver Polarization"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="revrpolrType">
		<xs:union memberTypes="FGDCstring">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="horizontal"/>
					<xs:enumeration value="vertical"/>
					<xs:enumeration value="left circular"/>
					<xs:enumeration value="right circular"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:element name="sendpolr" type="sendpolrType">
		<xs:annotation>
			<xs:documentation>Polarization of radiation emitted as part of a measurement system.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Mandatory-if-applicable"/>
				<xs:attribute name="mdname" fixed="Sender Polarization"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="sendpolrType">
		<xs:union memberTypes="FGDCstring">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="horizontal"/>
					<xs:enumeration value="vertical"/>
					<xs:enumeration value="left circular"/>
					<xs:enumeration value="right circular"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:element name="banddesc" type="FGDCstring">
		<xs:annotation>
			<xs:documentation>Additional descriptive material about band properties.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Optional"/>
				<xs:attribute name="mdname" fixed="Band Description"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="othcolde" type="FGDCstring">
		<xs:annotation>
			<xs:documentation>Description of properties of data collection system other than frame camera or whiskbroom scanner.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Mandatory"/>
				<xs:attribute name="mdname" fixed="Other Collector Description"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="instpdes" type="FGDCstring">
		<xs:annotation>
			<xs:documentation>Textual information on instrument properties, in areas not otherwise specified in this standard.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Optional"/>
				<xs:attribute name="mdname" fixed="Instrument Properties Description"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="instref" type="instrefType">
		<xs:annotation>
			<xs:documentation>Reference providing description of instrument properties and behavior.</xs:documentation>
			<xs:appinfo>
				<xs:attribute name="use" fixed="Mandatory"/>
				<xs:attribute name="mdname" fixed="Instrument Reference"/>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="instrefType">
		<xs:sequence>
			<xs:element name="citeinfo" type="citeinfoType"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
